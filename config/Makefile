#	@export BASHBIN=$(SHELL) && $(SHELL) progress_bar.sh $@ $$PPID &
	@echo "$(nl_)`date`$(nl_)" >logs/$@
	@ : > envars
	@$(call touch_timestamp)
	@$(call remove_existing_dirs2,gnutls-3.8.3.tar.xz)
	@$(PRT_DU_CR) >>logs/$@
	@$(call unpack2,gnutls-3.8.3.tar.xz)
	@$(call get_pkg_root2)
	@echo "export MAKEFLAGS=\"-j4\"" >> envars
	@echo "export NINJAJOBS=\"4\"" >> envars
	@echo "export CPPFLAGS=\"-D_FORTIFY_SOURCE=2 -DNDEBUG\"" >> envars
	@echo "export CFLAGS=\"-march=x86-64-v3 -mtune=haswell -O2 -pipe -fno-plt -fPIC -ffunction-sections -fdata-sections\"" >> envars
	@echo "export CXXFLAGS=\"-march=x86-64-v3 -mtune=haswell -O2 -pipe -fno-plt -fPIC -ffunction-sections -fdata-sections -Wp,-D_GLIBCXX_ASSERTIONS\"" >> envars
	@echo "export LDFLAGS=\"-Wl,-O2,--sort-common,--as-needed,-z,relro,-z,now,--gc-sections\"" >> envars
	@echo "export OTHER_CPPFLAGS=\"-D_FORTIFY_SOURCE=2 -DNDEBUG\"" >> envars
	@echo "export OTHER_CFLAGS=\"-march=x86-64-v3 -mtune=haswell -O2 -pipe -fno-plt -fPIC -ffunction-sections -fdata-sections\"" >> envars
	@echo "export OTHER_CXXFLAGS=\"-march=x86-64-v3 -mtune=haswell -O2 -pipe -fno-plt -fPIC -ffunction-sections -fdata-sections -Wp,-D_GLIBCXX_ASSERTIONS\"" >> envars
	@echo "export OTHER_LDFLAGS=\"-Wl,-O2,--sort-common,--as-needed,-z,relro,-z,now,--gc-sections\"" >> envars
	@source envars && \
	$(crCMDSDIR)/custom-tools/$@ >>$(crLOGDIR)/$@ 2>&1
	@$(call remove_build_dirs2,gnutls)
	@$(call log_new_files,Cus-022-gnutls)
	@touch /jhalfs/cus/gnutls-3.8.3
	@$(call housekeeping)

